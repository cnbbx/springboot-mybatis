<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cnbbx.mapper.UserMapper">

    <select id="findUserInfo" resultType="com.cnbbx.model.User">
        select name, age,password from userinfo limit 1;
    </select>

    <select id="findUsers" resultType="com.cnbbx.model.User">
        select name, age,password from userinfo;
    </select>

</mapper>